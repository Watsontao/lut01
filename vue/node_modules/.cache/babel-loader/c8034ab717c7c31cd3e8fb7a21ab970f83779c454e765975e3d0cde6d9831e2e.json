{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport VChart from 'vue-echarts';\nimport * as echarts from 'echarts/core';\nimport { LineChart } from 'echarts/charts';\nimport { TitleComponent, TooltipComponent, GridComponent, DatasetComponent, LegendComponent, DataZoomComponent, ToolboxComponent } from 'echarts/components';\nimport { CanvasRenderer } from 'echarts/renderers';\necharts.use([LineChart, TitleComponent, TooltipComponent, GridComponent, DatasetComponent, LegendComponent, DataZoomComponent, ToolboxComponent, CanvasRenderer]);\n\n/* ---------- 工具 ---------- */\nfunction formatDateLabel(v) {\n  const d = new Date(v);\n  const M = d.getMonth() + 1; // 去掉年份及前导 0\n  const D = d.getDate();\n  const h = ('0' + d.getHours()).slice(-2);\n  const m = ('0' + d.getMinutes()).slice(-2);\n  return `${M}-${D} ${h}:${m}`;\n}\nfunction buildYAxis(u) {\n  return {\n    type: 'value',\n    name: u,\n    axisLabel: {\n      formatter: `{value} ${u}`\n    },\n    max: v => v.max * 1.1,\n    minInterval: 1\n  };\n}\nfunction grad(c) {\n  return {\n    type: 'linear',\n    x: 0,\n    y: 0,\n    x2: 0,\n    y2: 1,\n    colorStops: [{\n      offset: 0,\n      color: c\n    }, {\n      offset: 1,\n      color: `${c}22`\n    } // 透明尾色\n    ]\n  };\n}\n\n// 导出数据为 csv / xlsx (极简)\nfunction exportData(format, name, labels, values) {\n  const rows = [['时间', name]].concat(labels.map((t, i) => [t, values[i]]));\n  let blob, ext;\n  if (format === 'csv') {\n    const csv = rows.map(r => r.join(',')).join('\\r\\n');\n    blob = new Blob([csv], {\n      type: 'text/csv;charset=utf-8'\n    });\n    ext = '.csv';\n  } else {\n    // very thin XML Spreadsheet 2003\n    const xmlRows = rows.map(r => `<Row>${r.map(c => `<Cell><Data ss:Type=\"String\">${c}</Data></Cell>`).join('')}</Row>`).join('');\n    const xml = `<?xml version=\"1.0\"?><Workbook xmlns=\"urn:schemas-microsoft-com:office:spreadsheet\"\n                  xmlns:ss=\"urn:schemas-microsoft-com:office:spreadsheet\"><Worksheet ss:Name=\"${name}\">\n                  <Table>${xmlRows}</Table></Worksheet></Workbook>`;\n    blob = new Blob([xml], {\n      type: 'application/vnd.ms-excel'\n    });\n    ext = '.xlsx';\n  }\n  const url = URL.createObjectURL(blob);\n  const a = document.createElement('a');\n  a.href = url;\n  a.download = name + ext;\n  a.click();\n  URL.revokeObjectURL(url);\n}\n/* ---------- /工具 ---------- */\n\nexport default {\n  name: 'GreenhouseDetail',\n  components: {\n    VChart\n  },\n  data() {\n    return {\n      greenhouse: {},\n      temperatureChart: {},\n      humidityChart: {},\n      soilChart: {},\n      lightChart: {},\n      /* 设备状态本地标记（实际以接口返回为准） */\n      irrigation: false,\n      ventilation: false,\n      lighting: false\n    };\n  },\n  created() {\n    const id = this.$route.query.id;\n    this.loadGreenhouse(id);\n    this.loadEnvData(id);\n    this.loadDeviceStatus(id);\n  },\n  methods: {\n    /* ---- UI ---- */\n    handleVideoError() {\n      this.$message.error('无信号，请检查摄像设备');\n    },\n    /* ---- 后端交互 ---- */\n    loadGreenhouse(id) {\n      this.$request.get(`/api/greenhouse/getById?id=${id}`).then(res => {\n        this.greenhouse = res.data || {};\n        if (this.greenhouse.videoUrl && !/^https?:/.test(this.greenhouse.videoUrl)) {\n          this.greenhouse.videoUrl = this.$baseUrl + this.greenhouse.videoUrl;\n        }\n      });\n    },\n    loadDeviceStatus(id) {\n      this.$request.get(`/api/device/status?greenhouseId=${id}`).then(res => {\n        const st = res.data || {};\n        this.irrigation = !!st.irrigation;\n        this.ventilation = !!st.ventilation;\n        this.lighting = !!st.lighting;\n      });\n    },\n    switchDevice(type) {\n      // 兼容 JDK8：不使用 List.of\n      const valid = ['irrigation', 'ventilation', 'lighting'];\n      if (valid.indexOf(type) === -1) {\n        this.$message.error('参数错误');\n        return;\n      }\n      const id = this.$route.query.id;\n      this.$request.post('/api/device/switch', {\n        greenhouseId: id,\n        type\n      }).then(res => {\n        this.$message.success(res.msg || '执行成功');\n        this.loadDeviceStatus(id); // 刷新状态\n      });\n    },\n\n    /* ---- 图表数据 ---- */\n    mkToolbox(seriesName, labels, dataArr) {\n      return {\n        right: 20,\n        feature: {\n          saveAsImage: {},\n          myCSV: {\n            title: '下载 CSV',\n            icon: 'path://M896 704H640v-64h256V128H128v512h256v64H128c-35.296 0-64-28.704-64-64V128c0-35.296 28.704-64 64-64h768c35.296 0 64 28.704 64 64v512c0 35.296-28.704 64-64 64z M704 768l128 128 128-128h-96V448h-64v320H704z',\n            onclick: () => exportData('csv', seriesName, labels, dataArr)\n          },\n          myXLSX: {\n            title: '下载 XLSX',\n            icon: 'path://M704 768l128 128 128-128h-96V448h-64v320H704z M896 704H640v-64h256V128H128v512h256v64H128c-35.296 0-64-28.704-64-64V128c0-35.296 28.704-64 64-64h768c35.296 0 64 28.704 64 64v512c0 35.296-28.704 64-64 64z',\n            onclick: () => exportData('xlsx', seriesName, labels, dataArr)\n          }\n        }\n      };\n    },\n    loadEnvData(id) {\n      this.$request.get(`/api/environment/history/${id}`).then(res => {\n        const rows = res.data || [];\n        const dates = rows.map(r => r.recordDate);\n        const xAxis = {\n          type: 'category',\n          data: dates,\n          axisLabel: {\n            formatter: formatDateLabel\n          }\n        };\n        const zoom = {\n          dataZoom: [{\n            type: 'inside',\n            xAxisIndex: 0\n          }, {\n            type: 'slider',\n            xAxisIndex: 0\n          }]\n        };\n\n        /* 温度 */\n        const temp = rows.map(r => r.temperature);\n        this.temperatureChart = {\n          color: [grad('#ff7f50')],\n          tooltip: {\n            trigger: 'axis'\n          },\n          xAxis,\n          yAxis: buildYAxis('℃'),\n          ...zoom,\n          toolbox: this.mkToolbox('温度', dates, temp),\n          series: [{\n            name: '温度',\n            type: 'line',\n            smooth: true,\n            data: temp\n          }]\n        };\n\n        /* 湿度 */\n        const hum = rows.map(r => r.humidity);\n        this.humidityChart = {\n          color: [grad('#1e90ff')],\n          tooltip: {\n            trigger: 'axis'\n          },\n          xAxis,\n          yAxis: buildYAxis('%'),\n          ...zoom,\n          toolbox: this.mkToolbox('湿度', dates, hum),\n          series: [{\n            name: '湿度',\n            type: 'line',\n            smooth: true,\n            data: hum\n          }]\n        };\n\n        /* 土壤湿度 */\n        const soil = rows.map(r => r.soilMoisture);\n        this.soilChart = {\n          color: [grad('#3cb371')],\n          tooltip: {\n            trigger: 'axis'\n          },\n          xAxis,\n          yAxis: buildYAxis('%'),\n          ...zoom,\n          toolbox: this.mkToolbox('土壤湿度', dates, soil),\n          series: [{\n            name: '土壤湿度',\n            type: 'line',\n            smooth: true,\n            data: soil\n          }]\n        };\n\n        /* 光照强度 */\n        const light = rows.map(r => r.lightIntensity);\n        this.lightChart = {\n          color: [grad('#ffa500')],\n          tooltip: {\n            trigger: 'axis'\n          },\n          xAxis,\n          yAxis: buildYAxis('lx'),\n          ...zoom,\n          toolbox: this.mkToolbox('光照强度', dates, light),\n          series: [{\n            name: '光照强度',\n            type: 'line',\n            smooth: true,\n            data: light\n          }]\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["VChart","echarts","LineChart","TitleComponent","TooltipComponent","GridComponent","DatasetComponent","LegendComponent","DataZoomComponent","ToolboxComponent","CanvasRenderer","use","formatDateLabel","v","d","Date","M","getMonth","D","getDate","h","getHours","slice","m","getMinutes","buildYAxis","u","type","name","axisLabel","formatter","max","minInterval","grad","c","x","y","x2","y2","colorStops","offset","color","exportData","format","labels","values","rows","concat","map","t","i","blob","ext","csv","r","join","Blob","xmlRows","xml","url","URL","createObjectURL","a","document","createElement","href","download","click","revokeObjectURL","components","data","greenhouse","temperatureChart","humidityChart","soilChart","lightChart","irrigation","ventilation","lighting","created","id","$route","query","loadGreenhouse","loadEnvData","loadDeviceStatus","methods","handleVideoError","$message","error","$request","get","then","res","videoUrl","test","$baseUrl","st","switchDevice","valid","indexOf","post","greenhouseId","success","msg","mkToolbox","seriesName","dataArr","right","feature","saveAsImage","myCSV","title","icon","onclick","myXLSX","dates","recordDate","xAxis","zoom","dataZoom","xAxisIndex","temp","temperature","tooltip","trigger","yAxis","toolbox","series","smooth","hum","humidity","soil","soilMoisture","light","lightIntensity"],"sources":["src/views/manager/GreenhouseDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"detail-page\">\r\n    <!-- ================= 视频 + 按钮 ================= -->\r\n    <div class=\"video-area\" v-if=\"greenhouse.videoUrl\">\r\n      <!-- 监控视频 -->\r\n      <video\r\n          :src=\"greenhouse.videoUrl\"\r\n          controls\r\n          class=\"video-player\"\r\n          @error=\"handleVideoError\"\r\n      />\r\n\r\n      <!-- 三个统一尺寸的控制按钮 -->\r\n      <div class=\"ctrl-box\">\r\n        <el-button\r\n            class=\"ctrl-btn\"\r\n            type=\"success\"\r\n            icon=\"el-icon-video-play\"\r\n            @click=\"switchDevice('irrigation')\"\r\n        >\r\n          {{ irrigation ? '停止灌溉' : '开始灌溉' }}\r\n        </el-button>\r\n\r\n        <el-button\r\n            class=\"ctrl-btn\"\r\n            type=\"warning\"\r\n            icon=\"el-icon-flag\"\r\n            @click=\"switchDevice('ventilation')\"\r\n        >\r\n          {{ ventilation ? '关闭通风' : '开启通风' }}\r\n        </el-button>\r\n\r\n        <el-button\r\n            class=\"ctrl-btn\"\r\n            type=\"primary\"\r\n            icon=\"el-icon-lightning\"\r\n            @click=\"switchDevice('lighting')\"\r\n        >\r\n          {{ lighting ? '关闭灯光' : '开启灯光' }}\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ================= 图表区 ================= -->\r\n    <div class=\"charts\">\r\n      <!-- 第一行 -->\r\n      <div class=\"chart-row\">\r\n        <div class=\"chart-card\">\r\n          <h3>温度变化（℃）</h3>\r\n          <v-chart :option=\"temperatureChart\" class=\"chart\" />\r\n        </div>\r\n        <div class=\"chart-card\">\r\n          <h3>湿度变化（%）</h3>\r\n          <v-chart :option=\"humidityChart\" class=\"chart\" />\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 第二行 -->\r\n      <div class=\"chart-row\">\r\n        <div class=\"chart-card\">\r\n          <h3>土壤湿度（%）</h3>\r\n          <v-chart :option=\"soilChart\" class=\"chart\" />\r\n        </div>\r\n        <div class=\"chart-card\">\r\n          <h3>光照强度（lx）</h3>\r\n          <v-chart :option=\"lightChart\" class=\"chart\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport VChart from 'vue-echarts'\r\nimport * as echarts from 'echarts/core'\r\nimport { LineChart } from 'echarts/charts'\r\nimport {\r\n  TitleComponent,\r\n  TooltipComponent,\r\n  GridComponent,\r\n  DatasetComponent,\r\n  LegendComponent,\r\n  DataZoomComponent,\r\n  ToolboxComponent\r\n} from 'echarts/components'\r\nimport { CanvasRenderer } from 'echarts/renderers'\r\n\r\necharts.use([\r\n  LineChart,\r\n  TitleComponent,\r\n  TooltipComponent,\r\n  GridComponent,\r\n  DatasetComponent,\r\n  LegendComponent,\r\n  DataZoomComponent,\r\n  ToolboxComponent,\r\n  CanvasRenderer\r\n])\r\n\r\n/* ---------- 工具 ---------- */\r\nfunction formatDateLabel(v) {\r\n  const d = new Date(v)\r\n  const M = d.getMonth() + 1         // 去掉年份及前导 0\r\n  const D = d.getDate()\r\n  const h = ('0' + d.getHours()).slice(-2)\r\n  const m = ('0' + d.getMinutes()).slice(-2)\r\n  return `${M}-${D} ${h}:${m}`\r\n}\r\n\r\nfunction buildYAxis(u) {\r\n  return {\r\n    type: 'value',\r\n    name: u,\r\n    axisLabel: { formatter: `{value} ${u}` },\r\n    max: v => v.max * 1.1,\r\n    minInterval: 1\r\n  }\r\n}\r\n\r\nfunction grad(c) {\r\n  return {\r\n    type: 'linear',\r\n    x: 0,\r\n    y: 0,\r\n    x2: 0,\r\n    y2: 1,\r\n    colorStops: [\r\n      { offset: 0, color: c },\r\n      { offset: 1, color: `${c}22` } // 透明尾色\r\n    ]\r\n  }\r\n}\r\n\r\n// 导出数据为 csv / xlsx (极简)\r\nfunction exportData(format, name, labels, values) {\r\n  const rows = [['时间', name]].concat(\r\n      labels.map((t, i) => [t, values[i]])\r\n  )\r\n\r\n  let blob, ext\r\n  if (format === 'csv') {\r\n    const csv = rows.map(r => r.join(',')).join('\\r\\n')\r\n    blob = new Blob([csv], { type: 'text/csv;charset=utf-8' })\r\n    ext = '.csv'\r\n  } else {\r\n    // very thin XML Spreadsheet 2003\r\n    const xmlRows = rows\r\n        .map(\r\n            r =>\r\n                `<Row>${r\r\n                    .map(c => `<Cell><Data ss:Type=\"String\">${c}</Data></Cell>`)\r\n                    .join('')}</Row>`\r\n        )\r\n        .join('')\r\n    const xml = `<?xml version=\"1.0\"?><Workbook xmlns=\"urn:schemas-microsoft-com:office:spreadsheet\"\r\n                  xmlns:ss=\"urn:schemas-microsoft-com:office:spreadsheet\"><Worksheet ss:Name=\"${name}\">\r\n                  <Table>${xmlRows}</Table></Worksheet></Workbook>`\r\n    blob = new Blob([xml], {\r\n      type: 'application/vnd.ms-excel'\r\n    })\r\n    ext = '.xlsx'\r\n  }\r\n\r\n  const url = URL.createObjectURL(blob)\r\n  const a = document.createElement('a')\r\n  a.href = url\r\n  a.download = name + ext\r\n  a.click()\r\n  URL.revokeObjectURL(url)\r\n}\r\n/* ---------- /工具 ---------- */\r\n\r\nexport default {\r\n  name: 'GreenhouseDetail',\r\n  components: { VChart },\r\n  data() {\r\n    return {\r\n      greenhouse: {},\r\n\r\n      temperatureChart: {},\r\n      humidityChart: {},\r\n      soilChart: {},\r\n      lightChart: {},\r\n\r\n      /* 设备状态本地标记（实际以接口返回为准） */\r\n      irrigation: false,\r\n      ventilation: false,\r\n      lighting: false\r\n    }\r\n  },\r\n  created() {\r\n    const id = this.$route.query.id\r\n    this.loadGreenhouse(id)\r\n    this.loadEnvData(id)\r\n    this.loadDeviceStatus(id)\r\n  },\r\n  methods: {\r\n    /* ---- UI ---- */\r\n    handleVideoError() {\r\n      this.$message.error('无信号，请检查摄像设备')\r\n    },\r\n\r\n    /* ---- 后端交互 ---- */\r\n    loadGreenhouse(id) {\r\n      this.$request.get(`/api/greenhouse/getById?id=${id}`).then(res => {\r\n        this.greenhouse = res.data || {}\r\n        if (\r\n            this.greenhouse.videoUrl &&\r\n            !/^https?:/.test(this.greenhouse.videoUrl)\r\n        ) {\r\n          this.greenhouse.videoUrl = this.$baseUrl + this.greenhouse.videoUrl\r\n        }\r\n      })\r\n    },\r\n\r\n    loadDeviceStatus(id) {\r\n      this.$request\r\n          .get(`/api/device/status?greenhouseId=${id}`)\r\n          .then(res => {\r\n            const st = res.data || {}\r\n            this.irrigation = !!st.irrigation\r\n            this.ventilation = !!st.ventilation\r\n            this.lighting = !!st.lighting\r\n          })\r\n    },\r\n\r\n    switchDevice(type) {\r\n      // 兼容 JDK8：不使用 List.of\r\n      const valid = ['irrigation', 'ventilation', 'lighting']\r\n      if (valid.indexOf(type) === -1) {\r\n        this.$message.error('参数错误')\r\n        return\r\n      }\r\n\r\n      const id = this.$route.query.id\r\n      this.$request\r\n          .post('/api/device/switch', { greenhouseId: id, type })\r\n          .then(res => {\r\n            this.$message.success(res.msg || '执行成功')\r\n            t